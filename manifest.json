{
  "applications": {
    "gecko": {
      "id": "tuchat-notifier@appleprincess.io"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "./icon.png",
    "default_title": "TUChat",
    "default_popup": "./popup.html"
  },
  "icons": {
    "36": "./icon.png",
    "48": "./icon48.png",
    "128": "./icon128.png"
  },
  "manifest_version": 2,
  "name":"TUChat Notifier",
  "permissions": ["notifications", "tabs"],
  "version": "0.0.0.3"
}
